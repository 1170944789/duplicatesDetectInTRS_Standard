(format TRS)
(fun app 2)
(fun id 0)
(fun add 0)
(fun |0| 0)
(fun s 0)
(fun map 0)
(fun nil 0)
(fun cons 0)
(rule (app id x) x)
(rule (app add |0|) id)
(rule (app (app add (app s x)) y) (app s (app (app add x) y)))
