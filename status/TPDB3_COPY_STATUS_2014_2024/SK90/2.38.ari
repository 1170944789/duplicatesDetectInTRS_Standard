(format TRS)
(fun ++ 2)
(fun nil 0)
(fun . 2)
(rule (++ nil y) y)
(rule (++ x nil) x)
(rule (++ (. x y) z) (. x (++ y z)))
(rule (++ (++ x y) z) (++ x (++ y z)))
