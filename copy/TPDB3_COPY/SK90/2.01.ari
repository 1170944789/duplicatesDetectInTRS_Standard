(format TRS)
(fun i 1)
(fun |0| 0)
(fun + 2)
(rule (i |0|) |0|)
(rule (+ |0| y) y)
(rule (+ x |0|) x)
(rule (i (i x)) x)
(rule (+ (i x) x) |0|)
(rule (+ x (i x)) |0|)
(rule (i (+ x y)) (+ (i x) (i y)))
(rule (+ x (+ y z)) (+ (+ x y) z))
(rule (+ (+ x (i y)) y) x)
(rule (+ (+ x y) (i y)) x)
