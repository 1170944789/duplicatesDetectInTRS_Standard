(format TRS)
(fun f 3)
(fun true 0)
(fun gt 2)
(fun trunc 1)
(fun s 1)
(fun |0| 0)
(fun false 0)
(rule (f true x y) (f (gt x y) (trunc x) (s y)))
(rule (trunc |0|) |0|)
(rule (trunc (s |0|)) |0|)
(rule (trunc (s (s x))) (s (s (trunc x))))
(rule (gt |0| v) false)
(rule (gt (s u) |0|) true)
(rule (gt (s u) (s v)) (gt u v))
