(format TRS)
(fun rev 1)
(fun nil 0)
(fun ++ 2)
(fun rev1 2)
(fun rev2 2)
(rule (rev nil) nil)
(rule (rev (++ x y)) (++ (rev1 x y) (rev2 x y)))
(rule (rev1 x nil) x)
(rule (rev1 x (++ y z)) (rev1 y z))
(rule (rev2 x nil) nil)
(rule (rev2 x (++ y z)) (rev (++ x (rev (rev2 y z)))))
