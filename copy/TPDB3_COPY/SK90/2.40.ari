(format TRS)
(fun or 2)
(fun true 0)
(fun false 0)
(fun mem 2)
(fun nil 0)
(fun set 1)
(fun = 2)
(fun union 2)
(rule (or true y) true)
(rule (or x true) true)
(rule (or false false) false)
(rule (mem x nil) false)
(rule (mem x (set y)) (= x y))
(rule (mem x (union y z)) (or (mem x y) (mem x z)))
