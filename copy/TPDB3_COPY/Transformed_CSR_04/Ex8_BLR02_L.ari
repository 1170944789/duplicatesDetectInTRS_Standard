(format TRS)
(fun sel 2)
(fun s 1)
(fun cons 1)
(fun fib 1)
(fun fib1 2)
(fun |0| 0)
(fun add 2)
(rule (sel (s N) (cons X)) (sel N XS))
(rule (fib N) (sel N (fib1 (s |0|) (s |0|))))
(rule (fib1 X Y) (cons X))
(rule (add |0| X) X)
(rule (add (s X) Y) (s (add X Y)))
(rule (sel |0| (cons X)) X)
