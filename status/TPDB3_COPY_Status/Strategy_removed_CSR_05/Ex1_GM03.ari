(format TRS)
(fun p 1)
(fun |0| 0)
(fun s 1)
(fun leq 2)
(fun true 0)
(fun false 0)
(fun if 3)
(fun diff 2)
(rule (p |0|) |0|)
(rule (p (s X)) X)
(rule (leq |0| Y) true)
(rule (leq (s X) |0|) false)
(rule (leq (s X) (s Y)) (leq X Y))
(rule (if true X Y) X)
(rule (if false X Y) Y)
(rule (diff X Y) (if (leq X Y) |0| (s (diff (p X) Y))))
