(format TRS)
(fun if 3)
(fun true 0)
(fun false 0)
(fun u 0)
(fun v 0)
(rule (if true x y) x)
(rule (if false x y) y)
(rule (if x y y) y)
(rule (if (if x y z) u v) (if x (if y u v) (if z u v)))
