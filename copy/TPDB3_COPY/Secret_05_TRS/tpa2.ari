(format TRS)
(fun - 2)
(fun |0| 0)
(fun s 1)
(fun p 1)
(fun f 2)
(rule (- x |0|) x)
(rule (- (s x) (s y)) (- x y))
(rule (p (s x)) x)
(rule (f (s x) y) (f (p (- (s x) y)) (p (- y (s x)))))
(rule (f x (s y)) (f (p (- x (s y))) (p (- (s y) x))))
