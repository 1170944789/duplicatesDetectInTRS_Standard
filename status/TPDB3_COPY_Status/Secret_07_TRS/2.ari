(format TRS)
(fun h 2)
(fun a 0)
(fun b 0)
(fun g 2)
(fun f 1)
(rule (h x x) (h a b))
(rule (g (g x a) y) (g (g a y) (g a x)))
(rule (f (g x y)) (g (g (f (f y)) (h a a)) x))
(rule (h (h (f (f x)) y) (h z v)) (h (h (f z) (f (f (f y)))) (h v x)))
