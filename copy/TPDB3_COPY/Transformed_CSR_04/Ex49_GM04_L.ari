(format TRS)
(fun if 1)
(fun true 0)
(fun false 0)
(fun minus 0)
(fun |0| 0)
(fun geq 0)
(fun div 1)
(fun s 1)
(rule (if true) X)
(rule (if false) Y)
(rule minus |0|)
(rule minus minus)
(rule geq true)
(rule geq false)
(rule geq geq)
(rule (div |0|) |0|)
(rule (div (s X)) (if geq))
