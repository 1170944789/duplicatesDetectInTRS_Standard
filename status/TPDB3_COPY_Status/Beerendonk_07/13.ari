(format TRS)
(fun cond1 3)
(fun true 0)
(fun cond2 3)
(fun gr 2)
(fun |0| 0)
(fun false 0)
(fun p 1)
(fun s 1)
(rule (cond1 true x y) (cond2 (gr x y) x y))
(rule (cond2 true x y) (cond1 (gr x |0|) y y))
(rule (cond2 false x y) (cond1 (gr x |0|) (p x) y))
(rule (gr |0| x) false)
(rule (gr (s x) |0|) true)
(rule (gr (s x) (s y)) (gr x y))
(rule (p |0|) |0|)
(rule (p (s x)) x)
