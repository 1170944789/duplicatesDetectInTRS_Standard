(format TRS)
(fun f 1)
(fun cons 2)
(fun nil 0)
(fun copy 3)
(fun n 0)
(fun |0| 0)
(fun s 1)
(rule (f (cons nil y)) y)
(rule (f (cons (f (cons nil y)) z)) (copy n y z))
(rule (copy |0| y z) (f z))
(rule (copy (s x) y z) (copy x y (cons (f y) z)))
