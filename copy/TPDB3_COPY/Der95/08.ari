(format TRS)
(fun D 1)
(fun t 0)
(fun |1| 0)
(fun constant 0)
(fun |0| 0)
(fun + 2)
(fun * 2)
(fun - 2)
(rule (D t) |1|)
(rule (D constant) |0|)
(rule (D (+ x y)) (+ (D x) (D y)))
(rule (D (* x y)) (+ (* y (D x)) (* x (D y))))
(rule (D (- x y)) (- (D x) (D y)))
