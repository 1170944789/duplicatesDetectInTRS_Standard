(format TRS)
(fun rev 1)
(fun nil 0)
(fun ++ 2)
(fun . 2)
(fun make 1)
(rule (rev nil) nil)
(rule (rev (rev x)) x)
(rule (rev (++ x y)) (++ (rev y) (rev x)))
(rule (++ nil y) y)
(rule (++ x nil) x)
(rule (++ (. x y) z) (. x (++ y z)))
(rule (++ x (++ y z)) (++ (++ x y) z))
(rule (make x) (. x nil))
