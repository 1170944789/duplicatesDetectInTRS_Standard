(format TRS)
(fun zeros 0)
(fun cons 2)
(fun |0| 0)
(fun n__zeros 0)
(fun tail 1)
(fun activate 1)
(rule zeros (cons |0| n__zeros))
(rule (tail (cons X XS)) (activate XS))
(rule zeros n__zeros)
(rule (activate n__zeros) zeros)
(rule (activate X) X)
