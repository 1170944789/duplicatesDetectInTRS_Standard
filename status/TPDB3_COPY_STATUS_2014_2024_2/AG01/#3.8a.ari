(format TRS)
(fun pred 1)
(fun s 1)
(fun minus 2)
(fun |0| 0)
(fun quot 2)
(fun log 1)
(rule (pred (s x)) x)
(rule (minus x |0|) x)
(rule (minus x (s y)) (pred (minus x y)))
(rule (quot |0| (s y)) |0|)
(rule (quot (s x) (s y)) (s (quot (minus x y) (s y))))
(rule (log (s |0|)) |0|)
(rule (log (s (s x))) (s (log (s (quot x (s (s |0|)))))))
